{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
        "dependsOn": ["^build"],
        "outputs": [
          "dist/**",
          ".next/**",
          "build/**",
          ".vercel/output/**"
        ]
     },
     "dev": {
        "dependsOn": ["^lint"]
     },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "clean": {
      "cache": false,
      "persistent": true
    }
  }
}
